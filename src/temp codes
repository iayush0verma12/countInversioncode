        int max=Integer.MIN_VALUE;
        for(int i=nums.length-1;i>=0;i--)
        {
            num[p]=nums[i];
            p++;
        }
        int count=0;
        for(int i=0;i<num.length;i++)
        {
            count=0;
            ArrayList<Integer>al=new ArrayList<>();
            int a=num[i];
            for(int j=i;j<num.length;j++)
            {
                int b=num[j];
                al.add(a-b);
            }
            for(int l=0;l<al.size();l++)
            {
                if(al.get(l)<=k)
                {
                    count++;
                    k=k-al.get(l);
                }
            }
            max=Math.max(count,max);
            k=K;
        }
        return max;



