    int N, M;
      cin >> N >> M;
      vector<int> colors(M);
      for (int i = 0; i < M; i++)
      {
        cin >> colors[i];
      }

      sort(colors.begin(), colors.end());

      int min_boxes = 0;
      int curr_boxes = 0;

      for (int i = 0; i < M; i++)
      {
        curr_boxes += colors[i];
        if (curr_boxes >= M)
        {
          min_boxes++;
          curr_boxes = 0;
        }
      }

      cout << min_boxes << endl;




