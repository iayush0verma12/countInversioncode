#include <iostream>
#include <bits/stdc++.h>

int main()
{
    int numTests;
    cin >> numTests;
    while (numTests--)
    {
        int length;
        cin >> length;
        string inputString;
        cin >> inputString;
        if (length % 3 == 1)
        {
            cout << "Yes\n";
            continue;
        }
        bool flag = false;
        for (char ch = 'a'; ch <= 'z'; ch++)
        {
            int firstIndex = length + 1, lastIndex = -1;
            for (int i = 0; i < length; i++)
            {
                if (i % 3 == 0 && inputString[i] == ch)
                {
                    firstIndex = i;
                    break;
                }
            }
            for (int i = length - 1; i >= 0; i--)
            {
                int x = length - 1 - i;
                if (x % 3 == 0 && inputString[i] == ch)
                {
                    lastIndex = i;
                    break;
                }
            }

            if (firstIndex < lastIndex)
            {
                flag = true;
                break;
            }
        }
        if (flag)
            cout << "Yes\n";
        else
            cout << "No\n";
    }
    return 0;
}



